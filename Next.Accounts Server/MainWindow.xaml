<Window x:Class="Next.Accounts_Server.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Next.Accounts_Server"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="36"/>
            <RowDefinition/>
            <RowDefinition Height="36"/>
        </Grid.RowDefinitions>
        <Menu x:Name="MainMenu" Background="#FFC5DCFD" Margin="0,0,0,5"/>
        <TextBox x:Name="LogTextBox" Margin="10" TextWrapping="Wrap" Text="TextBox" Grid.Row="1"/>
        <StatusBar Grid.Row="2" Margin="0,3,0,0">
            <Label x:Name="label" Content="Label" Width="157"/>
        </StatusBar>

    </Grid>
</Window>
