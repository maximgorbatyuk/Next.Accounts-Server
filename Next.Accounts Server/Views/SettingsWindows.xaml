<Window x:Class="Next.Accounts_Server.Windows.SettingsWindows"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Next.Accounts_Server.Windows"
        mc:Ignorable="d"
        Title="SettingsWindows" Height="300" Width="583.287">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="38"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="232" VerticalAlignment="Top" Width="575" Grid.RowSpan="2">
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5" Margin="0,0,2,-5"/>
            </TabItem>
        </TabControl>
        <Button x:Name="CancelButton" Content="Cancel" Margin="0,9,10,9" Grid.Row="1" HorizontalAlignment="Right" Width="89"/>
        <Button x:Name="ApplyButton" Content="Apply" Margin="0,9,105,9" Grid.Row="1" HorizontalAlignment="Right" Width="89"/>
    </Grid>
</Window>
